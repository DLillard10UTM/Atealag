        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="{Binding miscBonus, Mode=TwoWay}" VerticalAlignment="Top" Width="39" Margin="144,50,0,0"/>
        <TextBlock x:Name="add" HorizontalAlignment="Left" Margin="104,50,0,0" TextWrapping="Wrap" Text="+" VerticalAlignment="Top" Height="22" Width="9"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="22,50,0,0" TextWrapping="Wrap" Text="{Binding abilityBonus}" VerticalAlignment="Top" Height="22" Width="60"/>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="18,13,0,0" TextWrapping="Wrap" Text="Dex Modifier:" VerticalAlignment="Top" Width="67"/>
        <TextBlock x:Name="textBlock2" HorizontalAlignment="Left" Margin="149,29,0,0" TextWrapping="Wrap" Text="Misc:" VerticalAlignment="Top"/>
    </Grid>
</Window>
