<Window x:Class="Atealag.SpeedCalc"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Atealag"
        mc:Ignorable="d"
        Title="SpeedCalc" Height="110" Width="451">
    <Grid>
        <TextBox x:Name="BaseSpeed" HorizontalAlignment="Left" Height="23" Margin="160,28,0,0" TextWrapping="Wrap" Text="{Binding baseSpeed, Mode= TwoWay}" VerticalAlignment="Top" Width="47"/>
        <TextBox x:Name="MiscSpeed" HorizontalAlignment="Left" Height="23" Margin="251,28,0,0" TextWrapping="Wrap" Text="{Binding miscSpeed, Mode= TwoWay}" VerticalAlignment="Top" Width="47"/>

        <TextBlock x:Name="addition" HorizontalAlignment="Left" Margin="224,28,0,0" TextWrapping="Wrap" Text="+" VerticalAlignment="Top"/>
    </Grid>
</Window>
